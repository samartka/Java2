<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>user-info</servlet-name>
    <servlet-class>com.company.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>user-info</servlet-name>
    <url-pattern>/user-info/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>user-info-upgrade</servlet-name>
    <url-pattern>/user-info-upgrade/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>user-info-upgrade</servlet-name>
    <servlet-class>com.company.UserUpgradeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>com.company.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>codecamp-browser</servlet-name>
    <servlet-class>com.company.ClientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>codecamp-browser</servlet-name>
    <url-pattern>/codecamp-browser/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>instructor-servlet</servlet-name>
    <servlet-class>com.company.InstructorServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>instructor-servlet</servlet-name>
    <url-pattern>/instructor-servlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>user-login-servlet</servlet-name>
    <servlet-class>com.company.UserLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>user-login-servlet</servlet-name>
    <url-pattern>/user-login-servlet/*</url-pattern>
  </servlet-mapping>
</web-app>
